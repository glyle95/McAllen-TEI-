                <sch:let name="placeIDs"
                        value="$indexDoc//tei:body/tei:listPlace/tei:place/@xml:id"/>
                <sch:let name="placePtrs" value="for $i in $placeIDs return concat('#', $i)"/>
                <sch:assert test=". = $placePtrs"> Value must be one of the following: <sch:value-of select="string-join($placePtrs, ', ')"/>
                </sch:assert>
              </sch:rule>
         </pattern>
         <ref name="att.datable.attributes"/>
         <ref name="att.editLike.attributes"/>
         <ref name="att.global.attributes"/>
         <ref name="att.personal.attributes"/>
         <ref name="att.typed.attributes"/>
         <empty/>
      </element>
   </define>
